#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

     
    
    
    int arr[20],i,size,temp;
    printf("enter the size of array\n");
   
    scanf("%d",&size);
    printf("enter the element of array\n");
    for(i=0;i<size;i++)
    {
         scanf("%d",&arr[i]);
    }
    
     printf("reverse the array\n");
 temp=size-1;
    for(i=0;i<size;i++)
    {
       
      printf("%d ",arr[temp]);
     temp--;
     
    }
   
    return 0;
}
