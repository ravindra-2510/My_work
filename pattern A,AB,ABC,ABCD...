/* print pattern according size
A
AB
ABC
ABCD    */
#include <stdio.h>

int main()
{
   int size,i,j;
   scanf("%d",&size);
   //patteren code
   for(i=0;i<size;i++)
   {
       for(j=0;j<=i;j++)
       {
            printf("%c", 'A'+j);// increasing char using ascii value
       }
        printf("\n");
   }
  

    return 0;
}
