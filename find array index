/* Sample Input 0

5
6
2 4 6 9 6
Sample Output 0

2 */

#include <stdio.h>
int main()
{
   int size,i,j;
   int search;
   scanf("%d",&size);
   int arr[size];
   scanf("%d",&search);
   //scan element of array
   
   for(i=0;i<size;i++)
   {
       scanf("%d",&arr[i]);
   }
   
  //find index of array 
   for(i=0;i<size;i++)
   {
       if(arr[i]==search)
       {
       printf("\t %d",i);
       break; //after find search element first index break the loop;
       }
    
   }
